{
	"name": "remote-wsl",
	"displayName": "%displayName%",
	"description": "%description%",
	"version": "0.32.0",
	"publisher": "ms-vscode-remote",
	"icon": "resources/remote-wsl.png",
	"preview": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-remote-release.git"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-remote-release"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"engines": {
		"vscode": "^1.34.0"
	},
	"extensionKind": "ui",
	"categories": [
		"Other"
	],
	"keywords": [
		"remote",
		"wsl",
		"remote development"
	],
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"precommit": "node build/gulpfile.hygiene.js",
		"compile": "webpack --mode none",
		"watch": "webpack --mode none --watch",
		"package": "vsce package --yarn",
		"postversion": "git push && git push --tags",
		"patch-insiders": "yarn compile && node ./build/patch-local.js"
	},
	"activationEvents": [
		"onResolveRemoteAuthority:wsl",
		"onCommand:remote-wsl.newWindow",
		"onCommand:remote-wsl.showLog",
		"onCommand:remote-wsl.getHelp",
		"onCommand:remote-wsl.getStarted",
		"onCommand:remote-wsl.provideFeedback",
		"onCommand:remote-wsl.reportIssue"
	],
	"main": "./dist/extension",
	"enableProposedApi": true,
	"devDependencies": {
		"@types/node": "^8.10.25",
		"@types/webpack": "^4.4.27",
		"copy-webpack-plugin": "^5.0.2",
		"event-stream": "^4.0.1",
		"gulp": "^4.0.0",
		"gulp-filter": "^5.1.0",
		"husky": "^1.3.1",
		"p-all": "^2.1.0",
		"ts-loader": "^5.3.3",
		"tslint": "^5.15.0",
		"typescript": "^3.4.1",
		"typescript-formatter": "^7.2.2",
		"vinyl": "^2.2.0",
		"vinyl-fs": "^3.0.3",
		"vsce": "^1.59.0",
		"vscode": "^1.1.33",
		"vscode-nls-dev": "^3.2.6",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0"
	},
	"contributes": {
		"resourceLabelFormatters": [
			{
				"scheme": "vscode-remote",
				"authority": "wsl+*",
				"formatting": {
					"label": "${path}",
					"separator": "/",
					"tildify": true,
					"workspaceSuffix": "WSL"
				}
			}
		],
		"commands": [
			{
				"command": "remote-wsl.newWindow",
				"title": "New Window",
				"category": "Remote-WSL"
			},
			{
				"command": "remote-wsl.showLog",
				"title": "Show Log",
				"category": "Remote-WSL"
			},
			{
				"command": "remote-wsl.getHelp",
				"title": "Help",
				"category": "Remote-WSL"
			},
			{
				"command": "remote-wsl.getStarted",
				"title": "Getting Started",
				"category": "Remote-WSL"
			},
			{
				"command": "remote-wsl.provideFeedback",
				"title": "Provide Feedback",
				"category": "Remote-WSL"
			},
			{
				"command": "remote-wsl.reportIssue",
				"title": "Report Issue...",
				"category": "Remote-WSL"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "remote-wsl.newWindow",
					"when": "isWindows"
				},
				{
					"command": "remote-wsl.showLog",
					"when": "isWindows"
				},
				{
					"command": "remote-wsl.getHelp",
					"when": "isWindows"
				},
				{
					"command": "remote-wsl.getStarted",
					"when": "false"
				},
				{
					"command": "remote-wsl.provideFeedback",
					"when": "isWindows"
				},
				{
					"command": "remote-wsl.reportIssue",
					"when": "isWindows"
				}
			],
			"statusBar/windowIndicator": [
				{
					"command": "remote-wsl.newWindow",
					"when": "!remoteAuthority && isWindows",
					"group": "0_local_wsl@1"
				},
				{
					"command": "remote-wsl.getStarted",
					"when": "!remoteAuthority && isWindows",
					"group": "0_local_wsl@2"
				},
				{
					"command": "remote-wsl.newWindow",
					"when": "remoteAuthority =~ /^wsl\\+.*$/",
					"group": "1_remote_wsl_open@1"
				},
				{
					"command": "remote-wsl.showLog",
					"when": "remoteAuthority =~ /^wsl\\+.*$/",
					"group": "1_remote_wsl_open@2"
				},
				{
					"command": "remote-wsl.reportIssue",
					"when": "remoteAuthority =~ /^wsl\\+.*$/",
					"group": "9_remote_wsl_help@1"
				},
				{
					"command": "remote-wsl.provideFeedback",
					"when": "remoteAuthority =~ /^wsl\\+.*$/",
					"group": "9_remote_wsl_help@2"
				},
				{
					"command": "remote-wsl.getHelp",
					"when": "remoteAuthority =~ /^wsl\\+.*$/",
					"group": "9_remote_wsl_help@3"
				}
			]
		}
	},
	"__metadata": {
		"id": "f0c5397b-d357-4197-99f0-cb4202f22818",
		"publisherId": "ac9410a2-0d75-40ec-90de-b59bb705801d",
		"publisherDisplayName": "Microsoft"
	}
}